      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": "src/Domain/**.csproj",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "cwd": "..",
          "src": "../"
        }
      ],
      "dest": "api/Domain",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": "src/Infrastructure/**.csproj",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "cwd": "..",
          "src": "../"
        }
      ],
      "dest": "api/Infrastructure",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": "src/Persistence/**.csproj",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "cwd": "..",
          "src": "../"
        }
      ],
      "dest": "api/Persistence",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": "src/WebUI/**.csproj",
