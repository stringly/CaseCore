{
  "metadata": [
    {
      "src": [
        {
          "files": "src/Application/**.csproj",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "cwd": "..",
          "src": "../"
        }
      ],
      "dest": "api/Application",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
